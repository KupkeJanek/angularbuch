{
  "name": "project-manager-ssr",
  "version": "0.0.0",
  "license": "MIT",
  "repository": {
    "type": "git"
  },
  "scripts": {
    "ng": "ng",
    "start:client": "concurrently \"node projects-server/server.js db=projects-server/db.json\" \"ng serve\" ",
    "build": "ng build",
    "lint": "ng lint project-manager-ssr",
    "install-server": "npm install --prefix ./projects-server ./projects-server",
    "postinstall": "npm run install-server && npm run build:ssr",
    "build:prerender": "npm run build:client-and-server-bundles && npm run webpack:server && npm run generate:prerender",
    "build:client-and-server-bundles": "ng build --prod && ng run project-manager-ssr:server:production",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
    "build:ssr": "npm run build:client-and-server-bundles && npm run webpack:server",
    "generate:prerender": "cd dist && node prerender",
    "serve:prerender": "cd dist/browser && http-server",
    "serve:ssr": "node dist/server.js",
    "start:server": "concurrently \"node projects-server/server.js db=projects-server/db.json\" \"node dist/server.js\" ",
    "start": "node dist/server.js"
  },
  "pre-commit": [],
  "private": true,
